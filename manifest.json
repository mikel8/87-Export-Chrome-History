{
    "manifest_version": 3,
    "name": "My Chrome History",
    "version": "1.0",
    "description": "Export Chrome History as CSV",
    "permissions": [
      "history"
    ],
    "action": {
      "default_popup": "popup.html"
    },

    "icons": {
      "16": "th-16_x_16.png",
      "48": "th-48_x_48.png",
      "128": "th-128_x_128.png"
    }
  }
  